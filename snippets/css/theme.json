{
	"start_theme": {
		"prefix": "start_theme",
		"body": [
			"/**",
			" * Theme Name:        ${1:Theme Name}",
			" * Theme URI:         ${2:Theme URL Link}",
			" * Author:            ${3:Theme Author Name}",
			" * Author URI:        ${4:Theme Author Link}",
			" * Description:       ${5:This is Parent Theme X}",
			" * Version:           ${6:0.1.0}",
			" * Template:          ${7:0.1.0}",
			" * License:           ${8:GPL-2.0+}",
			" * License URL:       ${9:http://www.gnu.org/licenses/gpl-2.0.txt}",
			" * Text Domain:       ${10:prefix-theme-name}",
			" * Requires at least: ${11:4.9}",
			" * Requires PHP:      ${12:7.0}",
			" * Tested up to:      ${13:5.5}",
			" * Tags:              ${14:comma, seperated, tags }",
			"*/"
		],
		"description": "Theme start up details bootstrap"
	},
	"start_theme_child": {
		"prefix": "start_theme_child",
		"body": [
			"/**",
			" * Theme Name:  ${1:Child Theme Name}",
			" * Description: ${2:This Theme does wonders}",
			" * Author:      ${3:Theme Author Name}",
			" * Version:     ${4:0.1.0}",
			" * License:     ${5:GPL-2.0+}",
			" * Template:    ${6:parent-theme-name-text-domain}",
			"*/",
			"",
			"@import url('../${7:parent-theme-name}/style.css');"
		],
		"description": "Theme start up details bootstrap"
	}
}
